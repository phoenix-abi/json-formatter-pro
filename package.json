{
  "private": true,
  "name": "json-formatter-pro",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite build --watch",
    "build:content": "ENTRY=src/content.ts vite build",
    "build:rest": "EXCLUDE_CONTENT=1 vite build",
    "build": "npm run build:content && npm run build:rest",
    "test": "vitest run",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:contract": "vitest run tests/contract",
    "test:update": "UPDATE_SNAPSHOTS=1 vitest run",
    "test:e2e": "npm run build && playwright test tests/e2e/core",
    "test:e2e:perf": "npm run build && playwright test tests/e2e/performance",
    "test:e2e:preview": "npm run build && playwright test tests/e2e/preview",
    "test:e2e:all": "npm run build && playwright test tests/e2e",
    "test:perf": "node scripts/run-perf-with-output.mjs run --config vitest.config.perf.ts",
    "test:perf:smoke": "node scripts/run-perf-with-output.mjs run --config vitest.config.perf.ts tests/perf/parser-regression.smoke.spec.ts",
    "test:perf:parser": "node scripts/run-perf-with-output.mjs run --config vitest.config.perf.ts tests/perf/parser-comparison.spec.ts",
    "test:perf:parser:memory": "node scripts/run-perf-with-output.mjs --expose-gc node_modules/vitest/vitest.mjs run --config vitest.config.perf.ts tests/perf/parser-memory.spec.ts",
    "test:perf:render": "node scripts/run-perf-with-output.mjs run --config vitest.config.perf.ts tests/perf/render-buildDom.spec.ts tests/perf/render-preact.spec.tsx",
    "test:perf:interaction": "node scripts/run-perf-with-output.mjs run --config vitest.config.perf.ts tests/perf/interaction.spec.ts",
    "test:perf:all": "npm run test:perf && npm run test:perf:parser:memory",
    "test:all": "npm test && npm run test:e2e && npm run test:perf:smoke",
    "e2e:install": "playwright install --with-deps",
    "perf:compare": "node scripts/compare-perf.mjs",
    "perf:results": "node scripts/view-perf-results.mjs",
    "perf:results:list": "node scripts/view-perf-results.mjs --list",
    "perf:results:json": "node scripts/view-perf-results.mjs --json",
    "bundle:measure": "node scripts/measure-bundle.mjs",
    "style:preview:bundle": "vite build --config vite.config.preview.mjs",
    "style:preview": "node scripts/generate-style-test-page.mjs",
    "test:preview": "npm run style:preview && npm run test:e2e:preview",
    "clean": "rm -rf dist",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.3",
    "@playwright/test": "^1.49.0",
    "@preact/compat": "^18.3.1",
    "@preact/preset-vite": "^2.10.2",
    "@storybook/addon-a11y": "^10.1.9",
    "@storybook/addon-docs": "^10.1.9",
    "@storybook/addon-vitest": "^10.1.9",
    "@storybook/preact-vite": "^10.1.9",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/preact": "^3.2.4",
    "@testing-library/user-event": "^14.6.1",
    "@types/chrome": "^0.0.328",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^22.9.0",
    "@vitest/browser-playwright": "^4.0.14",
    "@vitest/coverage-v8": "^4.0.14",
    "fast-glob": "^3.3.2",
    "jsdom": "^27.0.1",
    "playwright": "^1.57.0",
    "storybook": "^10.1.9",
    "typescript": "^5.6.3",
    "vite": "^7.2.4",
    "vite-plugin-static-copy": "^3.1.4",
    "vitest": "^4.0.14"
  },
  "dependencies": {
    "preact": "^10.28.0",
    "react-window": "^2.2.3"
  }
}
